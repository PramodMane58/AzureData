# importing necessary libraries 
import pandas as pd
import numpy as np
from sklearn import datasets 
from sklearn.metrics import confusion_matrix 
from sklearn.cross_validation import train_test_split 
  
dataset1 = pd.read_csv('/home/admininistrator/Downloads/newHIV-1_data/746Data.txt', sep=",", header=None).head(5).values
print(dataset1)
allcolumns = list('ARNDCQEGHILKMFPSTWYV')
allcolumns = np.array(allcolumns)
new_list =[]
for index,row in dataset1:
    my_list = []
    for i in allcolumns:
        #print(index.count(i))
        my_list.append(index.count(i))
    new_list.append(my_list)
    #print(my_list)
    #col1 = list(i[0])l
    #for j in  i[,]
#new_list.insert(0,allcolumns)
X = np.array(new_list)
y = dataset1[:,1]
print(X)
print(allcolumns)
 #Splitting the dataset into the Training set and Test set
from sklearn.cross_validation import train_test_split
X_train, X_test, y_train, y_test = train_test_split(X, y, test_size = 0.25, random_state = 0)
from sklearn.tree import DecisionTreeClassifier
classifier = DecisionTreeClassifier(criterion = 'entropy', random_state = 0)
classifier.fit(X_train, y_train)
#Predicting the Test set results
y_pred = classifier.predict(X_test)
